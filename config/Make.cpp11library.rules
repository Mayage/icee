ifeq ($(CPP11),yes)

ifneq ($(LOCAL_DEPENDENT_DYMODULES),)
    LOCAL_DEPENDENT_DYMODULES := $(foreach lib, $(LOCAL_DEPENDENT_DYMODULES), \
        $(if $(filter $(lib),$(CPP11_MODULES)),$(lib)++11, $(lib)))
endif

ifneq ($(LOCAL_DEPENDENT_MODULES),)
    LOCAL_DEPENDENT_MODULES := $(foreach lib, $(LOCAL_DEPENDENT_MODULES),  \
        $(if $(filter $(lib),$(CPP11_MODULES)),$(lib)++11, $(lib)))
endif

endif
